<?xml version="1.0" encoding="UTF-8" ?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
	<meta>
		<author>John Purdum</author>
		<!--
		use "https://docs.google.com/uc?id=0B9B1n4-x6D2jZTlhYzU2MzMtN2I3Yy00YWRlLWFmMDAtMzEwOTgyMzlkYTBl&export=download&hl=en";select * from ee.finance.stock.profile where symbol = "GOOG";		
		-->
		<sampleQuery>select * from {table} where symbol = "YHOO"</sampleQuery>
	</meta>
	<bindings>
		<select itemPath="" produces="XML">
			<urls>
				<url>http://www.reuters.com/finance/stocks/companyProfile?symbol={symbol}</url>
			</urls>
			<inputs>				
				<key id='symbol' type='xs:string' paramType='path' required='true'/>
			</inputs>
			<execute><![CDATA[
				var results = y.query("select * from html where url=@url",{url:url});
				y.log(results);
      ]]></execute>
		</select>
	</bindings>
</table>